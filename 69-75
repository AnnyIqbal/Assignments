//task 8

var m = {
		Student_Name : prompt("Enter Name?"),
		Student_ID : prompt("Enter ID?"),
		Subject_HTML: prompt("Enter Marks in HTML?"),
		Student_CSS : prompt("Enter Marks in CSS?"),
		Student_JAVASCRIPT : prompt("Enter Marks in JAVASCRIPT?"),
		Student_BOOTSTRAP :  prompt("Enter Marks in BOOTSTRAP?"),
		//Total_Marks : parseInt(this.Subject_HTML) + parseInt(this.Student_CSS) + parseInt(this.Student_JAVASCRIPT) +parseInt(this.Student_BOOTSTRAP) 
	};

